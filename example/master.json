[
  {
    "key": "ctrl+o",
    "command": "workbench.action.gotoSymbol",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+alt+down",
    "command": "editor.action.copyLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+shift+c",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+p",
    "command": "editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },  
  {
    "key": "alt+shift+r",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+d",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "_info": "Back navigation, same as in browsers ( In vscode it is not supported currently, but behaves like this by default )",
    "key": ["alt+left", "mousebutton4"],
    "command": "workbench.action.navigateBack"
  },
  {
    "_info": "Forward navigation, same as in browsers ( In vscode it is not supported currently, but behaves like this by default )",
    "key": ["alt+right", "mousebutton5"],
    "command": "workbench.action.navigateForward"
  },
  {
    "key": "alt+z",
    "command": "editor.action.toggleWordWrap"
  },
  {
    "key": "ctrl+w",
    "command": "workbench.action.closeActiveEditor"
  },
  {
    "_info": "Join lines",
    "key": "ctrl+j",
    "command": "editor.action.joinLines"
  },
  {
    "key": "ctrl+t",
    "command": "editor.action.goToImplementation",
    "when": "editorHasImplementationProvider && editorTextFocus && !isInEmbeddedEditor"
  },
  {
    "key": "ctrl+alt+h",
    "command": "editor.showCallHierarchy",
    "when": "editorHasCallHierarchyProvider && editorTextFocus"
  },
  {
    "key": "ctrl+tab",
    "command": "workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup"
  },
  {
    "key": "ctrl+shift+n",
    "command": "workbench.files.action.showActiveFileInExplorer"
  },
  {
    "_info": "Reopen closed editor, same as in browsers",
    "key": "ctrl+shift+t",
    "command": "workbench.action.reopenClosedEditor"
  },
  {
    "_tool": "vscode",
    "key": "f12",
    "command": "workbench.files.action.showActiveFileInExplorer",
    "when": "isInDiffEditor"
  },
  {
    "key": "ctrl+alt+i",
    "command": "editor.action.formatDocument",
    "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "alt+t",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "alt+f",
    "command": "workbench.view.explorer"
  },
  {
    "key": "escape",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus || explorerViewletFocus || inlineChatFocus || chatInputHasFocus"
  },
  {
    "_info": "In vscode the search hides the explorer, so escape should bring back the explorer",
    "_tool": "vscode",
    "key": "escape",
    "command": "workbench.view.explorer",
    "when": "searchViewletFocus"
  },
  {
    "_info": "Quick file open",
    "key": "ctrl+shift+r",
    "command": "workbench.action.quickOpen"
  },
  {
    "_info": "Open GitHub Copilot Chat or focus if already opened",
    "key": "alt+a",
    "command": "workbench.panel.chat.view.copilot.focus"
  },
  {
    "_info": "Go to pairing bracket",
    "key": "alt+p",
    "command": "editor.action.jumpToBracket",
    "when": "editorTextFocus"
  },
  {
    "_info": "Open palette/all search",
    "key": "ctrl+shift+p",
    "command": "workbench.action.showCommands"
  },
  {
    "_info": "Open serach in files",
    "key": "ctrl+shift+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "_info": "Open find in editor",
    "key": "ctrl+f",
    "command": "actions.find"
  },
  {
    "_info": "Open find and replace in editor",
    "_tool": "intellij",
    "key": "ctrl+f",
    "command": "native:Replace"
  }
]
